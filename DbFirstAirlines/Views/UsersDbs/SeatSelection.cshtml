<style>
    body {
        min-height: 100vh;
        background-color: #f5f5f5;
        font-family: "Helvetica Neue",Arial,"Lucida Grande",sans-serif;
        font-size: 15px;
        perspective: 900px;
        transform-style: preserve-3d;
    }
    .plane {
        transform-style: preserve-3d;
    }
    .cabin {
        max-width: 500px;
        min-width: 450px;
        background-color: #e0e0e0;
        width: 80vw;
        margin: 25px auto;
        transition: transform 1s ease;
        transform: rotateX(0deg) translateZ(-500px) translateY(-200px);
        transform-style: preserve-3d;
    }
    .seat-row {
        display: flex;
        transform-style: preserve-3d;
        transition: background-color .24s ease;
    }
    .seat {
        width: 48px;
        height: 48px;
        margin: 9px;
    }
    .label1 {
        background-color: #757575;
        display: block;
        height: 100%;
        position: relative;
    }
    .seat-label {
        display: block;
        height: 100%;
        font-size: 0;
        position: relative;
}
    .aisle {
        flex: 1;
        position: relative;
}
    
    .btn-close {
        transform: scale(0);
        transition: transform .12s ease 0s;
    }
    table {
        margin-top: 9px;
    }
    caption {
        font-weight: 500;
        margin-bottom: 6px;
    }
    th, td {
        padding: 3px;
        font-size: 12px;
        line-height: 1.4;
    }
